{
  "expo": {
    "name": "crypto-price-tracker",
    "slug": "crypto-price-tracker",
    "platforms": ["ios", "android", "web"],
    "description": "Cryptocurrencies' prices tracker with React Native.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#282828"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cleversamer.cryptoPriceTracker",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.cleversamer.cryptoPriceTracker",
      "versionCode": 1
    },
    "web": {
      "favicon": "./app/assets/icon.png"
    }
  }
}
